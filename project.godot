config_version=5

[application]

config/name="CoPlayForge - Tactical RPG"
config/description="A tactical RPG inspired by Shining Force with low-stat characters that rely on group synergy and AI-driven behavior"
run/main_scene="res://scenes/Main_fixed.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/singletons/GameManager.gd"
CombatManager="*res://scripts/singletons/CombatManager.gd"
AudioManager="*res://scripts/singletons/AudioManager.gd"
HeroClassManager="*res://scripts/singletons/HeroClassManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="CoPlayForge - Tactical RPG"

